{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "IsHangfireEnabled": false,
  "RabbitMqUri": "rabbitmq://127.0.0.1:5010",
  "RabbitMqUsername": "guest",
  "RabbitMqPassword": "guest",
  "BoltOnSamplesDbConnectionString": "Data Source=127.0.0.1,5005;initial catalog=BoltOnSamples;persist security info=True;User ID=sa;Password=Password1;",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Error",
        "System": "Error",
        "CorrelationId": "Error",
        "BoltOn": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "Path": "logs/applicationLog.txt",
          "OutputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff}] [{CorrelationId}] [{MachineName}] [{Level:u3}] [{SourceContext}] [{ClientIp}] [{RequestUrl}]> {Message:lj} {NewLine}{Exception}",
          "RollOnFileSizeLimit": "true",
          "FileSizeLimitBytes": "10485760",
          "RetainedFileCountLimit": "10"
        }
      }
    ]
  }
}
